    <plugin-info>
        <description>${dollar}{project.description}</description>
        <version>${dollar}{project.version}</version>
        <!-- TODO: Add vendor details -->
        <!--
        <vendor name="Example Company" url="http://www.example.com"/>
        -->
    </plugin-info>

    <servlet name="${artifactId}" class="${groupId}.ExampleServlet" key="test">
        <url-pattern>/test</url-pattern>
    </servlet>
</atlassian-plugin>
