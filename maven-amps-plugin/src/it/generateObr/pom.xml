<project>
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.atlassian.amps.it</groupId>
    <artifactId>maven-amps-unicorn-plugin-genenerate-obr-artifact-test</artifactId>
    <version>testing</version>
    <name>TestGenerateObrArtifact</name>

    <build>
        <plugins>
            <plugin>
                <groupId>com.atlassian.maven.plugins</groupId>
                <artifactId>maven-${shitty.product}-plugin</artifactId>
                <version>testing</version>
                <configuration>
                    <pluginDependencies>
                        <pluginDependency>
                            <groupId>commons-io</groupId>
                            <artifactId>commons-io</artifactId>
                        </pluginDependency>
                    </pluginDependencies>
                    <instructions>
                        <Bundle-SymbolicName>com.atlassian.amps.it.maven-amps-unicorn-plugin-genenerate-obr-artifact-test</Bundle-SymbolicName>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>commons-io</groupId>
            <artifactId>commons-io</artifactId>
            <version>1.4</version>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.1.1</version>
        </dependency>
    </dependencies>
</project>